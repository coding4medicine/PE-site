<h2>Show book</h2>

<ul>

  <li>
    <strong>Title:</strong>
    <%= @book.title %>
  </li>

  <li>
    <strong>Author:</strong>
    <%= @book.author %>
  </li>

  <li>
    <strong>Image:</strong>
    <%= @book.image %>
  </li>

  <li>
    <strong>Price:</strong>
    <%= @book.price %>
  </li>

  <li>
    <strong>Visible:</strong>
    <%= @book.visible %>
  </li>

</ul>

<%= link "Edit", to: book_path(@conn, :edit, @book) %>
<%= link "Back", to: book_path(@conn, :index) %>
